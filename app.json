{
  "expo": {
    "name": "Delivery",
    "slug": "Delivery",
    "scheme": "delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo/fast-shipping.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/Logo/deli.png"
    }
  }
}
